{
  "name": "marvel-stone-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node dist/Api/index.js",
    "babel:build": "babel src --extensions  \".js,.ts\" --out-dir dist --copy-files",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/Api/index.ts",
    "dev:server": "NODE_ENV=development ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/Api/index.ts",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "test:staged": "jest",
    "typeorm": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js --config ormconfig.js",
    "typeorm:dev": "NODE_ENV=development ts-node ./node_modules/typeorm/cli.js --config ormconfig.js",
    "run-scheme:build": "yarn typeorm schema:sync",
    "run-scheme:development": "yarn typeorm:dev schema:sync",
    "postinstall": "husky install",
    "swagger-autogen": "node src/swagger.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-unless": "^0.5.0",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "rate-limiter-flexible": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5",
    "supertest": "^6.1.3",
    "swagger-autogen": "^2.6.8",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-unless": "^0.5.1",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "@types/pg": "^7.14.10",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^5.1.1",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.2",
    "typescript": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix"
    ]
  }
}
